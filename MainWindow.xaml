<Controls:MetroWindow
    x:Class="V3.Build.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Controls="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro"
    xmlns:WPFConsole="clr-namespace:ConsoleControl.WPF;assembly=ConsoleControl.WPF"
    xmlns:converters="http://materialdesigninxaml.net/winfx/xaml/themes"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:iconPacks="http://metro.mahapps.com/winfx/xaml/iconpacks"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:md="http://materialdesigninxaml.net/winfx/xaml/themes"
    xmlns:simpleChildWindow="clr-namespace:MahApps.Metro.SimpleChildWindow;assembly=MahApps.Metro.SimpleChildWindow"
    xmlns:system="clr-namespace:System;assembly=mscorlib"
    Title="   V²"
    Width="1000"
    Height="700"
    BorderBrush="{DynamicResource MahApps.Brushes.Accent}"
    BorderThickness="2"
    FontFamily="{DynamicResource MaterialDesignFont}"
    GlowBrush="{DynamicResource MahApps.Brushes.Accent}"
    ResizeMode="CanResizeWithGrip"
    ShowMinButton="True"
    WindowStartupLocation="CenterOwner"
    mc:Ignorable="d">
    <Window.Resources>
        <ResourceDictionary>

            <ResourceDictionary.MergedDictionaries>

                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Dark.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Defaults.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignColors;component/Themes/Recommended/Primary/MaterialDesignColor.BlueGrey.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.PopupBox.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Defaults.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MahApps.Metro;component/Styles/Controls.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MahApps.Metro;component/Styles/Fonts.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MahApps.Metro;component/Styles/Themes/Dark.Steel.xaml" />

            </ResourceDictionary.MergedDictionaries>

            <Style x:Key="StatusLabel" TargetType="Label">
                <Setter Property="BorderThickness" Value="0" />
                <Setter Property="FontSize" Value="25" />
                <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                <Setter Property="Margin" Value="3,7,3,0" />
                <Setter Property="Content" Value="" />
                <Setter Property="ToolTip" Value="Component is missing or incorrectly configured" />
                <Setter Property="Foreground" Value="Red" />
                <Style.Triggers>
                    <Trigger Property="Tag" Value="True">
                        <Setter Property="Content" Value="" />
                        <Setter Property="Foreground" Value="Green" />
                        <Setter Property="ToolTip" Value="Component is correctly configured" />
                    </Trigger>
                </Style.Triggers>



            </Style>


            <Style
                x:Key="flyoutItemBox"
                BasedOn="{StaticResource MaterialDesignTextBox}"
                TargetType="{x:Type TextBox}">
                <Setter Property="BorderThickness" Value="0" />
                <Setter Property="FontSize" Value="12.5" />
                <Setter Property="IsReadOnly" Value="True" />
                <Setter Property="Width" Value="150" />
                <Setter Property="md:TextFieldAssist.NewSpecHighlightingEnabled" Value="False" />
                <Setter Property="md:HintAssist.Foreground" Value="{StaticResource MahApps.Brushes.SystemControlBackgroundBaseMediumLow}" />
                <Setter Property="md:TextFieldAssist.DecorationVisibility" Value="Hidden" />
                <Setter Property="Margin" Value="0,-14,0,0" />


            </Style>

            <Style BasedOn="{StaticResource {x:Type md:SnackbarMessage}}" TargetType="md:SnackbarMessage">
                <Setter Property="HorizontalContentAlignment" Value="Center" />
            </Style>

            <Style BasedOn="{StaticResource MaterialDesignFlatAccentBgButton}" TargetType="Button">
                <Setter Property="Padding" Value="0" />
                <Setter Property="BorderThickness" Value=".5" />
                <Setter Property="Background" Value="#2a2a2a" />
                <Setter Property="BorderBrush" Value="{DynamicResource MahApps.Brushes.Button.Border}" />
                <Setter Property="FontFamily" Value="{StaticResource MahApps.Fonts.Family.Header}" />


            </Style>

            <Style
                x:Key="logGridHeader"
                BasedOn="{StaticResource MahApps.Styles.DataGridColumnHeader}"
                TargetType="DataGridColumnHeader">
                <Setter Property="HorizontalAlignment" Value="Center" />
                <Setter Property="HorizontalContentAlignment" Value="Center" />
                <Setter Property="FontSize" Value="8" />
                <Setter Property="Margin" Value="-10,0,0,0" />
                <Style.Triggers>
                    <Trigger Property="IsMouseOver" Value="True">
                        <Setter Property="Background" Value="Transparent" />
                        <Setter Property="Cursor" Value="Arrow" />
                    </Trigger>

                </Style.Triggers>


            </Style>

            <Style
                x:Key="rtSwitch"
                BasedOn="{StaticResource MahApps.Styles.ToggleSwitch}"
                TargetType="Controls:ToggleSwitch">
                <Setter Property="Width" Value="75" />
                <Setter Property="Margin" Value="0,0,-15,0" />
                <Setter Property="DockPanel.Dock" Value="Right" />
                <Setter Property="Content" Value="" />
                <Setter Property="HorizontalAlignment" Value="Right" />
            </Style>
            <Style x:Key="rtHeader" TargetType="Label">
                <Setter Property="FontSize" Value="16" />
                <Setter Property="FontFamily" Value="{DynamicResource MahApps.Fonts.Family.Header}" />
                <Setter Property="Margin" Value="0,0,-15,0" />
                <Setter Property="DockPanel.Dock" Value="Top" />
            </Style>
            <Style x:Key="rtSubHeader" TargetType="TextBlock">
                <Setter Property="Margin" Value="5,0,0,0" />
                <Setter Property="FontFamily" Value="{DynamicResource MahApps.Fonts.Family.Header}" />
                <Setter Property="DockPanel.Dock" Value="Top" />
                <Setter Property="Foreground" Value="LightGray" />
            </Style>





            <Style
                x:Key="rtClick"
                BasedOn="{StaticResource MaterialDesignFlatAccentBgButton}"
                TargetType="Button">
                <Setter Property="Width" Value="34" />
                <Setter Property="Margin" Value="0,8,8,8" />
                <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                <Setter Property="Content" Value="" />
                <Setter Property="HorizontalAlignment" Value="Right" />
                <Setter Property="DockPanel.Dock" Value="Right" />
                <Setter Property="Padding" Value="0" />
                <Setter Property="BorderThickness" Value=".5" />
                <Setter Property="Background" Value="#2a2a2a" />
                <Setter Property="BorderBrush" Value="{DynamicResource MahApps.Brushes.Button.Border}" />
                <Setter Property="FontSize" Value="16" />
            </Style>

            <Style
                x:Key="rtClickDel"
                BasedOn="{StaticResource MaterialDesignFlatAccentBgButton}"
                TargetType="Button">
                <Setter Property="Width" Value="34" />
                <Setter Property="Margin" Value="0,8,8,8" />
                <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                <Setter Property="Content" Value="" />
                <Setter Property="HorizontalAlignment" Value="Right" />
                <Setter Property="DockPanel.Dock" Value="Left" />
                <Setter Property="Padding" Value="0" />
                <Setter Property="BorderThickness" Value=".5" />
                <Setter Property="Background" Value="#2a2a2a" />
                <Setter Property="BorderBrush" Value="{DynamicResource MahApps.Brushes.Button.Border}" />
                <Setter Property="FontSize" Value="16" />
            </Style>

            <Style x:Key="rtLabel" TargetType="Label">
                <Setter Property="Margin" Value="3,7,3,0" />
                <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                <Setter Property="Content" Value="" />
                <Setter Property="HorizontalAlignment" Value="Right" />
                <Setter Property="DockPanel.Dock" Value="Right" />
                <Setter Property="FontSize" Value="25" />
                <Setter Property="Foreground" Value="Yellow" />
            </Style>



            <Style x:Key="datagridColumn" TargetType="DataGridColumnHeader">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type DataGridColumnHeader}">
                            <TextBlock
                                Width="{TemplateBinding Width}"
                                Margin="0,5,0,0"
                                Padding="0"
                                FontWeight="DemiBold"
                                Foreground="White"
                                Text="{TemplateBinding Content}"
                                TextAlignment="Left" />
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>

            <Style
                x:Key="datagridColumnNoTab"
                BasedOn="{StaticResource datagridColumn}"
                TargetType="DataGridColumnHeader">
                <Setter Property="IsTabStop" Value="False" />
            </Style>




            <Style
                x:Key="actionbtn"
                BasedOn="{StaticResource MaterialDesignFlatAccentBgButton}"
                TargetType="Button">
                <Setter Property="Padding" Value="0" />
                <Setter Property="BorderThickness" Value=".5" />
                <Setter Property="Background" Value="#2a2a2a" />
                <Setter Property="BorderBrush" Value="{DynamicResource MahApps.Brushes.Button.Border}" />
                <Setter Property="FontFamily" Value="{StaticResource MahApps.Fonts.Family.Header}" />
                <Style.Triggers>
                    <DataTrigger Binding="{Binding PropName, Mode=OneWay}" Value="Non-AD Property">
                        <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                        <Setter Property="Content" Value="" />
                        <Setter Property="IsEnabled" Value="True" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding ActionName, Mode=OneWay}" Value="ReadOnly">
                        <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                        <Setter Property="Content" Value="" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding ActionName, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}" Value="Editable">
                        <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                        <Setter Property="Content" Value="" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding ActionName, Mode=OneWay}" Value="Actionable">
                        <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                        <Setter Property="Content" Value="" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding ActionName}" Value="Actionable-Raw">
                        <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                        <Setter Property="Content" Value="" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding ActionName}" Value="Editable-Actionable-Raw">
                        <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                        <Setter Property="Content" Value="" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding ActionName}" Value="Editable-Actionable">
                        <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                        <Setter Property="Content" Value="" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding ActionName}" Value="null">
                        <Setter Property="IsEnabled" Value="False" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding ActionName}" Value="ReadOnly-Raw">
                        <Setter Property="IsEnabled" Value="False" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding ActionName}" Value="Editable-Raw">
                        <Setter Property="IsEnabled" Value="False" />
                    </DataTrigger>
                </Style.Triggers>
            </Style>
            <Style
                x:Key="actiontoolbtn"
                BasedOn="{StaticResource MaterialDesignFlatAccentBgButton}"
                TargetType="Button">
                <Setter Property="Padding" Value="0" />
                <Setter Property="BorderThickness" Value=".5" />
                <Setter Property="Background" Value="#2a2a2a" />
                <Setter Property="BorderBrush" Value="{DynamicResource MahApps.Brushes.Button.Border}" />
                <Setter Property="FontFamily" Value="{StaticResource MahApps.Fonts.Family.Header}" />
                <Style.Triggers>
                    <DataTrigger Binding="{Binding toolType, Mode=OneWay}" Value="Execute">
                        <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                        <Setter Property="Content" Value="" />
                        <Setter Property="IsEnabled" Value="True" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding toolType, Mode=OneWay}" Value="Select">
                        <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                        <Setter Property="Content" Value="" />
                        <Setter Property="IsEnabled" Value="True" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding toolType, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}" Value="Grid">
                        <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                        <Setter Property="Content" Value="" />
                        <Setter Property="IsEnabled" Value="True" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding toolType, Mode=OneWay}" Value="List">
                        <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                        <Setter Property="Content" Value="" />
                        <Setter Property="IsEnabled" Value="True" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding toolType}" Value="null">
                        <Setter Property="IsEnabled" Value="False" />
                    </DataTrigger>

                </Style.Triggers>
            </Style>
            <Style
                x:Key="upbtn"
                BasedOn="{StaticResource MaterialDesignIconButton}"
                TargetType="Button">
                <Style.Triggers>
                    <DataTrigger Binding="{Binding topPos, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Value="True">
                        <Setter Property="Visibility" Value="Hidden" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding topPos, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Value="False">
                        <Setter Property="Visibility" Value="Visible" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding ID, Mode=OneWay}" Value="1">
                        <Setter Property="Visibility" Value="Hidden" />
                    </DataTrigger>

                </Style.Triggers>
            </Style>
            <Style
                x:Key="downbtn"
                BasedOn="{StaticResource MaterialDesignIconButton}"
                TargetType="Button">
                <Style.Triggers>
                    <DataTrigger Binding="{Binding ID, Mode=OneWay}" Value="1">
                        <Setter Property="Visibility" Value="Hidden" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding ID, Mode=OneWay}" Value="2">
                        <Setter Property="Visibility" Value="Hidden" />
                    </DataTrigger>
                </Style.Triggers>
            </Style>
            <Style
                x:Key="nameClear"
                BasedOn="{StaticResource MaterialDesignIconButton}"
                TargetType="Button">
                <Style.Triggers>
                    <DataTrigger Binding="{Binding ID, Mode=OneWay}" Value="1">
                        <Setter Property="Visibility" Value="Hidden" />
                    </DataTrigger>
                </Style.Triggers>
            </Style>






            <Style
                x:Key="customHeaderField"
                BasedOn="{StaticResource MahApps.Styles.TextBox}"
                TargetType="TextBox">
                <Setter Property="IsEnabled" Value="False" />
                <Style.Triggers>
                    <DataTrigger Binding="{Binding FieldSel}" Value="Custom">
                        <Setter Property="IsEnabled" Value="True" />
                    </DataTrigger>
                </Style.Triggers>
            </Style>



            <Style
                x:Key="dataGridRowStyle"
                BasedOn="{StaticResource MahApps.Styles.DataGridRow}"
                TargetType="DataGridRow">

                <Style.Triggers>
                    <MultiTrigger>
                        <MultiTrigger.Conditions>
                            <Condition Property="Controls:DataGridHelper.SelectionUnit" Value="FullRow" />
                            <Condition Property="IsSelected" Value="True" />
                        </MultiTrigger.Conditions>
                        <Setter Property="Background" Value="Transparent" />
                        <Setter Property="BorderBrush" Value="Transparent" />

                    </MultiTrigger>
                    <MultiTrigger>
                        <MultiTrigger.Conditions>
                            <Condition Property="Controls:DataGridHelper.SelectionUnit" Value="FullRow" />
                            <Condition Property="IsMouseOver" Value="True" />
                        </MultiTrigger.Conditions>
                        <Setter Property="Background" Value="Transparent" />
                        <Setter Property="BorderBrush" Value="Transparent" />

                    </MultiTrigger>
                    <!--  IsSelected  -->
                    <MultiTrigger>
                        <MultiTrigger.Conditions>
                            <Condition Property="Controls:DataGridHelper.SelectionUnit" Value="FullRow" />
                            <Condition Property="IsSelected" Value="True" />
                        </MultiTrigger.Conditions>
                        <Setter Property="Background" Value="Transparent" />
                        <Setter Property="BorderBrush" Value="Transparent" />

                    </MultiTrigger>

                    <!--  IsSelected and Selector.IsSelectionActive  -->
                    <MultiTrigger>
                        <MultiTrigger.Conditions>

                            <Condition Property="IsSelected" Value="True" />
                        </MultiTrigger.Conditions>
                        <Setter Property="Background" Value="Transparent" />
                        <Setter Property="BorderBrush" Value="Transparent" />
                    </MultiTrigger>

                </Style.Triggers>
            </Style>
            <Style
                x:Key="dataGridRowStyleLog"
                BasedOn="{StaticResource dataGridRowStyle}"
                TargetType="DataGridRow">
                <Setter Property="Background" Value="#FF363333" />
            </Style>
            <Style
                x:Key="dataGridRowResultsStyleLog"
                BasedOn="{StaticResource dataGridRowStyle}"
                TargetType="DataGridRow">
                <Setter Property="Background" Value="Transparent" />
            </Style>


            <Style
                x:Key="dataGridCell"
                BasedOn="{StaticResource MaterialDesignDataGridCell}"
                TargetType="DataGridCell">
                <Style.Setters>
                    <Setter Property="BorderThickness" Value="0" />

                    <Setter Property="IsTabStop" Value="False" />


                </Style.Setters>
                <Style.Triggers>
                    <Trigger Property="IsSelected" Value="True">
                        <Setter Property="Background" Value="Transparent" />
                        <Setter Property="Foreground" Value="Transparent" />
                    </Trigger>
                </Style.Triggers>

            </Style>

            <Style
                x:Key="dataGridCellLog"
                BasedOn="{StaticResource dataGridCell}"
                TargetType="DataGridCell">
                <Style.Setters>

                    <Setter Property="Background" Value="#FF363333" />
                </Style.Setters>
            </Style>
            <Style
                x:Key="dataGridCellResultsLog"
                BasedOn="{StaticResource dataGridCell}"
                TargetType="DataGridCell">
                <Style.Setters>

                    <Setter Property="Background" Value="Transparent" />
                </Style.Setters>
            </Style>
            <Style
                x:Key="itemBox"
                BasedOn="{StaticResource MaterialDesignTextBox}"
                TargetType="{x:Type TextBox}">

                <Setter Property="BorderThickness" Value="0" />
                <Setter Property="FontSize" Value="16" />
                <Setter Property="Grid.Column" Value="0" />
                <Setter Property="md:HintAssist.Hint" Value="[empty]" />
                <Setter Property="md:HintAssist.HintOpacity" Value=".15" />
                <Setter Property="MaxLength" Value="20" />
                <Setter Property="FontSize" Value="16" />
                <Setter Property="FontFamily" Value="{DynamicResource MaterialDesignFont}" />
                <Setter Property="md:TextFieldAssist.UnderlineBrush" Value="AliceBlue" />
                <Setter Property="md:TextFieldAssist.NewSpecHighlightingEnabled" Value="True" />
                <Setter Property="Controls:TextBoxHelper.AutoWatermark" Value="True" />
                <Setter Property="Controls:TextBoxHelper.Watermark" Value="Null" />
                <Setter Property="md:TextFieldAssist.DecorationVisibility" Value="Visible" />
                <Setter Property="Margin" Value="3,10,0,0" />
                <Setter Property="CaretBrush" Value="AliceBlue" />


                <Style.Triggers>
                    <Trigger Property="Tag" Value="NoEdit">
                        <Setter Property="md:TextFieldAssist.DecorationVisibility" Value="Hidden" />
                        <Setter Property="md:TextFieldAssist.NewSpecHighlightingEnabled" Value="False" />
                        <Setter Property="IsReadOnly" Value="True" />
                        <Setter Property="Cursor" Value="Arrow" />
                    </Trigger>
                </Style.Triggers>
            </Style>
            <Style
                x:Key="gridTextItem"
                BasedOn="{StaticResource MaterialDesignTextBox}"
                TargetType="{x:Type TextBox}">

                <Setter Property="BorderThickness" Value="0" />
                <Setter Property="md:HintAssist.Hint" Value="[none]" />
                <Setter Property="md:HintAssist.HintOpacity" Value=".15" />
                <Setter Property="md:TextFieldAssist.UnderlineBrush" Value="AliceBlue" />
                <Setter Property="md:TextFieldAssist.NewSpecHighlightingEnabled" Value="True" />
                <Setter Property="md:TextFieldAssist.DecorationVisibility" Value="Visible" />
                <Setter Property="md:TextFieldAssist.DecorationVisibility" Value="Hidden" />
                <Setter Property="md:TextFieldAssist.NewSpecHighlightingEnabled" Value="False" />
                <Setter Property="IsReadOnly" Value="True" />
                <Setter Property="HorizontalAlignment" Value="Left" />
                <Setter Property="Cursor" Value="Arrow" />


            </Style>
            <Style
                x:Key="compItemBox"
                BasedOn="{StaticResource MaterialDesignTextBoxBase}"
                TargetType="{x:Type TextBox}">
                <Setter Property="Background" Value="Transparent" />
                <Setter Property="md:TextFieldAssist.UnderlineBrush" Value="Transparent" />
                <Setter Property="md:TextFieldAssist.DecorationVisibility" Value="Hidden" />
                <Setter Property="md:TextFieldAssist.NewSpecHighlightingEnabled" Value="False" />
                <Setter Property="Margin" Value="4,-5,0,15" />
                <Setter Property="IsReadOnly" Value="True" />
                <Setter Property="Cursor" Value="Arrow" />
                <Setter Property="md:HintAssist.Hint" Value="[empty]" />
                <Setter Property="md:HintAssist.HintOpacity" Value=".15" />
                <Setter Property="MaxLength" Value="20" />
                <Setter Property="FontSize" Value="12" />
                <Setter Property="BorderThickness" Value="0" />
                <Setter Property="FontFamily" Value="{DynamicResource MaterialDesignFont}" />

            </Style>

            <Style x:Key="itemStackPanel" TargetType="StackPanel">
                <Setter Property="Grid.Column" Value="1" />
                <Setter Property="HorizontalAlignment" Value="Right" />
                <Setter Property="Orientation" Value="Horizontal" />



            </Style>

            <Style
                x:Key="customTabLabel"
                BasedOn="{StaticResource MaterialDesignLabel}"
                TargetType="{x:Type Label}">
                <Setter Property="FontSize" Value="26" />
                <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                <Setter Property="Foreground" Value="{DynamicResource MahApps.Brushes.Accent4}" />
                <Setter Property="Padding" Value="10" />


            </Style>



            <Style
                x:Key="itemBoxHeader"
                BasedOn="{StaticResource MaterialDesignLabel}"
                TargetType="{x:Type Label}">
                <Setter Property="Foreground" Value="{DynamicResource MahApps.Brushes.SystemControlBackgroundBaseMediumLow}" />
                <Setter Property="FontSize" Value="14" />
                <Setter Property="Margin" Value="-2,-6,0,0" />
                <Setter Property="FontFamily" Value="{DynamicResource md:MaterialDesignFont}" />



            </Style>

            <Style x:Key="itemViewBox" TargetType="{x:Type Viewbox}">
                <Setter Property="HorizontalAlignment" Value="Left" />

                <Setter Property="MaxWidth" Value="180" />
                <Setter Property="MaxHeight" Value="35" />
                <Setter Property="MinWidth" Value="30" />
                <Setter Property="MinHeight" Value="20" />
                <Setter Property="StretchDirection" Value="Both" />
                <Setter Property="Stretch" Value="Uniform" />
                <Setter Property="VerticalAlignment" Value="Bottom" />
            </Style>


            <Style x:Key="itemBorder" TargetType="{x:Type Border}">
                <Setter Property="Margin" Value="1" />
                <Setter Property="Padding" Value="1" />
                <Setter Property="Background" Value="{DynamicResource MahApps.Brushes.ThemeBackground}" />
                <Setter Property="BorderBrush" Value="{DynamicResource MahApps.Brushes.Accent}" />
                <Setter Property="BorderThickness" Value="1" />
                <Setter Property="CornerRadius" Value="3" />
            </Style>


            <Style x:Key="itemEditClip" TargetType="{x:Type Label}">
                <Setter Property="Margin" Value="0,-3,0,0" />
                <Setter Property="HorizontalAlignment" Value="Left" />
                <Setter Property="VerticalAlignment" Value="Top" />
                <Setter Property="DockPanel.Dock" Value="Left" />
                <Setter Property="Foreground" Value="AliceBlue" />
                <Setter Property="FontSize" Value="12" />
                <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                <Setter Property="Content" Value="" />

            </Style>

            <Style
                x:Key="itemButton"
                BasedOn="{StaticResource MaterialDesignRaisedButton}"
                TargetType="{x:Type Button}">
                <Setter Property="Grid.Column" Value="1" />
                <Setter Property="Height" Value="Auto" />
                <Setter Property="Width" Value="Auto" />
                <Setter Property="Height" Value="20" />
                <Setter Property="Width" Value="20" />
                <Setter Property="Margin" Value="3" />
                <Setter Property="Padding" Value="1.5" />
                <Setter Property="Foreground" Value="WhiteSmoke" />
                <Setter Property="FontSize" Value="14" />
                <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                <Setter Property="Background" Value="{DynamicResource MahApps.Brushes.Accent3}" />
                <Setter Property="BorderBrush" Value="{DynamicResource MahApps.Brushes.Accent}" />
                <Setter Property="DockPanel.Dock" Value="Left" />
            </Style>


            <Style
                x:Key="itemToolButton"
                BasedOn="{StaticResource itemButton}"
                TargetType="{x:Type Button}">
                <Setter Property="Width" Value="63" />
                <Setter Property="Height" Value="63" />
                <Setter Property="FontSize" Value="24" />
            </Style>

            <Style
                x:Key="itemEditButton"
                BasedOn="{StaticResource itemButton}"
                TargetType="{x:Type Button}">
                <Setter Property="Margin" Value="2" />
                <Setter Property="Content" Value="" />
            </Style>

            <Style
                x:Key="itemToolLargeButton"
                BasedOn="{StaticResource itemButton}"
                TargetType="{x:Type Button}">
                <Setter Property="Width" Value="63" />
                <Setter Property="Height" Value="63" />
                <Setter Property="FontSize" Value="24" />
                <Setter Property="Width" Value="63" />
            </Style>

            <Style x:Key="statusIconOnline" TargetType="{x:Type Label}">
                <Style.Setters>
                    <Setter Property="HorizontalAlignment" Value="Center" />
                    <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                </Style.Setters>
                <Style.Triggers>

                    <DataTrigger Binding="{Binding Connectivity, Mode=OneWay}" Value="True">

                        <Setter Property="Content" Value="" />
                        <Setter Property="Foreground" Value="LightGreen" />
                    </DataTrigger>

                    <DataTrigger Binding="{Binding Connectivity, Mode=OneWay}" Value="False">

                        <Setter Property="Content" Value="" />
                        <Setter Property="Foreground" Value="IndianRed" />
