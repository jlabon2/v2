<Controls:MetroWindow
    x:Class="V3.Build.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Controls="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro"
    xmlns:WPFConsole="clr-namespace:ConsoleControl.WPF;assembly=ConsoleControl.WPF"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:iconPacks="http://metro.mahapps.com/winfx/xaml/iconpacks"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:md="http://materialdesigninxaml.net/winfx/xaml/themes"
    xmlns:simpleChildWindow="clr-namespace:MahApps.Metro.SimpleChildWindow;assembly=MahApps.Metro.SimpleChildWindow"
    xmlns:sys="clr-namespace:System;assembly=mscorlib"
    Title="   V²"
    Width="1000"
    Height="700"
    BorderBrush="{DynamicResource MahApps.Brushes.Accent}"
    BorderThickness="2"
    FontFamily="Segoe UI"
    GlowBrush="{DynamicResource MahApps.Brushes.Accent}"
    ResizeMode="CanResizeWithGrip"
    ShowMinButton="True"
    TextElement.FontFamily="Segoe UI"
    WindowStartupLocation="CenterOwner"
    WindowTitleBrush="{DynamicResource MahApps.Brushes.Accent}"
    mc:Ignorable="d">

    <Window.Resources>
        <ResourceDictionary>

            <ResourceDictionary.MergedDictionaries>

                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Dark.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Defaults.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignColors;component/Themes/Recommended/Primary/MaterialDesignColor.BlueGrey.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.PopupBox.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Defaults.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MahApps.Metro;component/Styles/Controls.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MahApps.Metro;component/Styles/Fonts.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MahApps.Metro;component/Styles/Themes/Dark.Steel.xaml" />
            </ResourceDictionary.MergedDictionaries>

            <Style x:Key="StatusLabel" TargetType="Label">
                <Setter Property="BorderThickness" Value="0" />
                <Setter Property="FontSize" Value="25" />
                <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                <Setter Property="Margin" Value="3,7,3,0" />
                <Setter Property="Content" Value="" />
                <Setter Property="ToolTip" Value="Component is missing or incorrectly configured" />
                <Setter Property="Foreground" Value="Red" />
                <Style.Triggers>
                    <Trigger Property="Tag" Value="True">
                        <Setter Property="Content" Value="" />
                        <Setter Property="Foreground" Value="Green" />
                        <Setter Property="ToolTip" Value="Component is correctly configured" />
                    </Trigger>
                </Style.Triggers>
            </Style>

            <Style x:Key="varNameRun" TargetType="Run">
                <Setter Property="FontFamily" Value="Segoe UI" />
                <Setter Property="Foreground" Value="{StaticResource MahApps.Brushes.AccentBase}" />
                <Setter Property="FontSize" Value="11" />
            </Style>

            <Style x:Key="varDescRun" TargetType="Run">
                <Setter Property="FontFamily" Value="Segoe UI Light" />
                <Setter Property="Foreground" Value="GhostWhite" />
                <Setter Property="FontSize" Value="11" />
            </Style>

            <Style x:Key="ScriptBlockErrorStatus" TargetType="Label">
                <Setter Property="Foreground" Value="WhiteSmoke" />
                <Setter Property="Visibility" Value="Collapsed" />
                <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                <Setter Property="Content" Value="" />
                <Setter Property="Panel.ZIndex" Value="1" />
                <Setter Property="VerticalAlignment" Value="Center" />
                <Style.Triggers>
                    <Trigger Property="Tag" Value="Pass">
                        <Setter Property="Foreground" Value="LightGreen" />
                        <Setter Property="Visibility" Value="Visible" />
                    </Trigger>
                    <Trigger Property="Tag" Value="PassWithIssues">
                        <Setter Property="Foreground" Value="Yellow" />
                        <Setter Property="Visibility" Value="Visible" />
                    </Trigger>
                    <Trigger Property="Tag" Value="Fail">
                        <Setter Property="Foreground" Value="IndianRed" />
                        <Setter Property="Visibility" Value="Visible" />
                    </Trigger>
                </Style.Triggers>
            </Style>

            <Style
                x:Key="ErrorStatusTextBox"
                BasedOn="{StaticResource MaterialDesignOutlinedTextFieldTextBox}"
                TargetType="TextBox">
                <Setter Property="IsReadOnly" Value="True" />
                <Setter Property="md:HintAssist.Foreground" Value="White" />
                <Setter Property="md:HintAssist.Hint" Value="Scriptblock validity" />
                <Setter Property="TextWrapping" Value="NoWrap" />
                <Setter Property="VerticalContentAlignment" Value="Top" />
                <Style.Triggers>
                    <Trigger Property="Tag" Value="Unchecked">
                        <Setter Property="Text" Value="Scriptblock requires validation" />
                    </Trigger>
                    <Trigger Property="Tag" Value="Pass">
                        <Setter Property="Text" Value="Scriptblock is valid" />
                    </Trigger>
                    <Trigger Property="Tag" Value="PassWithIssues">
                        <Setter Property="Text" Value="Scriptblock is valid but has warnings" />
                    </Trigger>
                    <Trigger Property="Tag" Value="Fail">
                        <Setter Property="Text" Value="Scriptblock is invalid and has major errors" />
                    </Trigger>
                </Style.Triggers>
            </Style>
            <DrawingBrush
                x:Key="ThatchBackground"
                Stretch="None"
                TileMode="Tile"
                Viewport="0,0,15,15"
                ViewportUnits="Absolute">
                <DrawingBrush.Drawing>
                    <DrawingGroup>
                        <GeometryDrawing Brush="#FF363333">
                            <GeometryDrawing.Geometry>
                                <GeometryGroup>
                                    <RectangleGeometry Rect="0,0,60,60" />
                                    <RectangleGeometry Rect="60,60,60,60" />
                                </GeometryGroup>
                            </GeometryDrawing.Geometry>
                        </GeometryDrawing>
                        <GeometryDrawing Brush="#FF363338">
                            <GeometryDrawing.Geometry>
                                <GeometryGroup>
                                    <RectangleGeometry Rect="0,0,60,60" />
                                    <RectangleGeometry Rect="60,60,60,60" />
                                </GeometryGroup>
                            </GeometryDrawing.Geometry>
                        </GeometryDrawing>
                    </DrawingGroup>
                </DrawingBrush.Drawing>
            </DrawingBrush>
            <Style
                x:Key="flyoutItemBox"
                BasedOn="{StaticResource MaterialDesignTextBox}"
                TargetType="{x:Type TextBox}">
                <Setter Property="BorderThickness" Value="0" />
                <Setter Property="FontSize" Value="12.5" />
                <Setter Property="IsReadOnly" Value="True" />
                <Setter Property="Width" Value="150" />
                <Setter Property="md:TextFieldAssist.NewSpecHighlightingEnabled" Value="False" />
                <Setter Property="md:HintAssist.Foreground" Value="{StaticResource MahApps.Brushes.SystemControlBackgroundBaseMediumLow}" />
                <Setter Property="md:TextFieldAssist.DecorationVisibility" Value="Hidden" />
                <Setter Property="Margin" Value="0,-14,0,0" />
            </Style>

            <Style BasedOn="{StaticResource {x:Type md:SnackbarMessage}}" TargetType="md:SnackbarMessage">
                <Setter Property="HorizontalContentAlignment" Value="Center" />
            </Style>

            <Style BasedOn="{StaticResource MaterialDesignFlatAccentBgButton}" TargetType="Button">
                <Setter Property="Padding" Value="0" />
                <Setter Property="BorderThickness" Value=".5" />
                <Setter Property="Background" Value="#2a2a2a" />
                <Setter Property="BorderBrush" Value="{DynamicResource MahApps.Brushes.Button.Border}" />
                <Setter Property="FontFamily" Value="{StaticResource MahApps.Fonts.Family.Header}" />
            </Style>


            <Style
                x:Key="logGridHeader"
                BasedOn="{StaticResource MahApps.Styles.DataGridColumnHeader}"
                TargetType="DataGridColumnHeader">
                <Setter Property="HorizontalAlignment" Value="Center" />
                <Setter Property="HorizontalContentAlignment" Value="Center" />
                <Setter Property="FontSize" Value="8" />
                <Setter Property="Margin" Value="-10,0,0,0" />
                <Style.Triggers>
                    <Trigger Property="IsMouseOver" Value="True">
                        <Setter Property="Background" Value="Transparent" />
                        <Setter Property="Cursor" Value="Arrow" />
                    </Trigger>
                </Style.Triggers>
            </Style>

            <Style
                x:Key="historyGridHeader"
                BasedOn="{StaticResource logGridHeader}"
                TargetType="DataGridColumnHeader">
                <Setter Property="Foreground" Value="LightGray" />
                <Setter Property="FontSize" Value="10" />
                <Setter Property="BorderThickness" Value="0" />
                <Setter Property="Background" Value="Transparent" />
                <Setter Property="Margin" Value="-10,0,0,0" />
            </Style>

            <Style
                x:Key="rtSwitch"
                BasedOn="{StaticResource MahApps.Styles.ToggleSwitch}"
                TargetType="Controls:ToggleSwitch">
                <Setter Property="Width" Value="75" />
                <Setter Property="Margin" Value="0,0,-15,0" />
                <Setter Property="DockPanel.Dock" Value="Right" />
                <Setter Property="Content" Value="" />
                <Setter Property="HorizontalAlignment" Value="Right" />
            </Style>
            <Style x:Key="rtHeader" TargetType="Label">
                <Setter Property="FontSize" Value="16" />
                <Setter Property="FontFamily" Value="{DynamicResource MahApps.Fonts.Family.Header}" />
                <Setter Property="Margin" Value="0,0,-15,0" />
                <Setter Property="DockPanel.Dock" Value="Top" />
            </Style>
            <Style x:Key="rtSubHeader" TargetType="TextBlock">
                <Setter Property="Margin" Value="5,0,0,0" />
                <Setter Property="FontFamily" Value="{DynamicResource MahApps.Fonts.Family.Header}" />
                <Setter Property="DockPanel.Dock" Value="Top" />
                <Setter Property="Foreground" Value="LightGray" />
            </Style>

            <Style
                x:Key="rtClick"
                BasedOn="{StaticResource MaterialDesignFlatAccentBgButton}"
                TargetType="Button">
                <Setter Property="Width" Value="34" />
                <Setter Property="Margin" Value="0,8,8,8" />
                <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                <Setter Property="Content" Value="" />
                <Setter Property="HorizontalAlignment" Value="Right" />
                <Setter Property="DockPanel.Dock" Value="Right" />
                <Setter Property="Padding" Value="0" />
                <Setter Property="BorderThickness" Value=".5" />
                <Setter Property="Background" Value="#2a2a2a" />
                <Setter Property="BorderBrush" Value="{DynamicResource MahApps.Brushes.Button.Border}" />
                <Setter Property="FontSize" Value="16" />
            </Style>

            <Style
                x:Key="rtClickDel"
                BasedOn="{StaticResource MaterialDesignFlatAccentBgButton}"
                TargetType="Button">
                <Setter Property="Width" Value="34" />
                <Setter Property="Margin" Value="0,8,8,8" />
                <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                <Setter Property="Content" Value="" />
                <Setter Property="HorizontalAlignment" Value="Right" />
                <Setter Property="DockPanel.Dock" Value="Left" />
                <Setter Property="Padding" Value="0" />
                <Setter Property="BorderThickness" Value=".5" />
                <Setter Property="Background" Value="#2a2a2a" />
                <Setter Property="BorderBrush" Value="{DynamicResource MahApps.Brushes.Button.Border}" />
                <Setter Property="FontSize" Value="16" />
            </Style>

            <Style x:Key="rtLabel" TargetType="Label">
                <Setter Property="Margin" Value="3,7,3,0" />
                <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                <Setter Property="Content" Value="" />
                <Setter Property="HorizontalAlignment" Value="Right" />
                <Setter Property="DockPanel.Dock" Value="Right" />
                <Setter Property="FontSize" Value="25" />
                <Setter Property="Foreground" Value="Yellow" />
            </Style>

            <Style x:Key="datagridColumn" TargetType="DataGridColumnHeader">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type DataGridColumnHeader}">
                            <TextBlock
                                Width="{TemplateBinding Width}"
                                Margin="0,5,0,0"
                                Padding="0"
                                FontWeight="DemiBold"
                                Foreground="White"
                                Text="{TemplateBinding Content}"
                                TextAlignment="Left" />
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>

            <Style
                x:Key="datagridColumnNoTab"
                BasedOn="{StaticResource datagridColumn}"
                TargetType="DataGridColumnHeader">
                <Setter Property="IsTabStop" Value="False" />
            </Style>
            <Style x:Key="FlyoutSeparator" TargetType="Separator">
                <Setter Property="RenderTransform">
                    <Setter.Value>
                        <ScaleTransform ScaleY="2" />
                    </Setter.Value>
                </Setter>
                <Setter Property="Background" Value="{StaticResource MahApps.Brushes.Badged.Background.Disabled}" />
                <Style.Triggers>
                    <DataTrigger Binding="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type simpleChildWindow:ChildWindow}}, Path=IsWindowHostActive}" Value="True">
                        <Setter Property="Background" Value="{StaticResource MahApps.Brushes.Accent}" />
                    </DataTrigger>
                </Style.Triggers>
            </Style>
            <Style
                x:Key="actionbtn"
                BasedOn="{StaticResource MaterialDesignFlatAccentBgButton}"
                TargetType="Button">
                <Setter Property="Padding" Value="0" />
                <Setter Property="BorderThickness" Value=".5" />
                <Setter Property="Background" Value="#2a2a2a" />
                <Setter Property="BorderBrush" Value="{DynamicResource MahApps.Brushes.Button.Border}" />
                <Setter Property="FontFamily" Value="{StaticResource MahApps.Fonts.Family.Header}" />
                <Style.Triggers>
                    <DataTrigger Binding="{Binding PropName, Mode=OneWay}" Value="Non-AD Property">
                        <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                        <Setter Property="Content" Value="" />
                        <Setter Property="IsEnabled" Value="True" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding ActionName, Mode=OneWay}" Value="ReadOnly">
                        <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                        <Setter Property="Content" Value="" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding ActionName, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}" Value="Editable">
                        <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                        <Setter Property="Content" Value="" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding ActionName, Mode=OneWay}" Value="Actionable">
                        <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                        <Setter Property="Content" Value="" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding ActionName}" Value="Actionable-Raw">
                        <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                        <Setter Property="Content" Value="" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding ActionName}" Value="Editable-Actionable-Raw">
                        <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                        <Setter Property="Content" Value="" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding ActionName}" Value="Editable-Actionable">
                        <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                        <Setter Property="Content" Value="" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding ActionName}" Value="null">
                        <Setter Property="IsEnabled" Value="False" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding ActionName}" Value="ReadOnly-Raw">
                        <Setter Property="IsEnabled" Value="False" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding ActionName}" Value="Editable-Raw">
                        <Setter Property="IsEnabled" Value="False" />
                    </DataTrigger>
                </Style.Triggers>
            </Style>

            <Style x:Key="customItemText" TargetType="TextBlock">
                <Setter Property="Margin" Value="0,15,0,0" />
                <Setter Property="FontFamily" Value="Segoe UI Light" />
                <Setter Property="FontSize" Value="12" />
                <Setter Property="Foreground" Value="White" />
                <Style.Triggers>
                    <Trigger Property="Tag" Value="AD Object (any)">
                        <Setter Property="Text" Value="AD Object Selection:" />
                    </Trigger>
                    <Trigger Property="Tag" Value="AD User">
                        <Setter Property="Text" Value="AD User Selection:" />
                    </Trigger>
                    <Trigger Property="Tag" Value="AD Computer">
                        <Setter Property="Text" Value="AD Computer Selection:" />
                    </Trigger>
                    <Trigger Property="Tag" Value="AD Group">
                        <Setter Property="Text" Value="AD Group Selection:" />
                    </Trigger>
                    <Trigger Property="Tag" Value="OU">
                        <Setter Property="Text" Value="ou Selection:" />
                    </Trigger>
                    <Trigger Property="Tag" Value="String">
                        <Setter Property="Text" Value="Text Selection:" />
                    </Trigger>
                    <Trigger Property="Tag" Value="Choice">
                        <Setter Property="Text" Value="Choice Selection:" />
                    </Trigger>
                    <Trigger Property="Tag" Value="Integer">
                        <Setter Property="Text" Value="Number Selection:" />
                    </Trigger>
                    <Trigger Property="Tag" Value="File">
                        <Setter Property="Text" Value="File Path Selection:" />
                    </Trigger>
                    <Trigger Property="Tag" Value="Directory">
                        <Setter Property="Text" Value="Folder Path Selection:" />
                    </Trigger>
                </Style.Triggers>

            </Style>

            <Style
                x:Key="actionbtnstatic"
                BasedOn="{StaticResource MaterialDesignFlatAccentBgButton}"
                TargetType="Button">
                <Setter Property="Padding" Value="0" />
                <Setter Property="BorderThickness" Value=".5" />
                <Setter Property="Background" Value="#2a2a2a" />
                <Setter Property="BorderBrush" Value="{DynamicResource MahApps.Brushes.Button.Border}" />
                <Setter Property="FontFamily" Value="{StaticResource MahApps.Fonts.Family.Header}" />
            </Style>
            <Style
                x:Key="actiontoolbtn"
                BasedOn="{StaticResource MaterialDesignFlatAccentBgButton}"
                TargetType="Button">
                <Setter Property="Padding" Value="0" />
                <Setter Property="BorderThickness" Value=".5" />
                <Setter Property="Background" Value="#2a2a2a" />
                <Setter Property="Content" Value="" />
                <Setter Property="IsEnabled" Value="False" />
                <Setter Property="BorderBrush" Value="{StaticResource MahApps.Brushes.Button.Border}" />
                <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                <Style.Triggers>
                    <DataTrigger Binding="{Binding toolType, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}" Value="Execute">
                        <Setter Property="Content" Value="" />
                        <Setter Property="IsEnabled" Value="True" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding toolType, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}" Value="Select">
                        <Setter Property="Content" Value="" />
                        <Setter Property="IsEnabled" Value="True" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding toolType, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}" Value="Grid">
                        <Setter Property="Content" Value="" />
                        <Setter Property="IsEnabled" Value="True" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding toolType, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}" Value="List">
                        <Setter Property="Content" Value="" />
                        <Setter Property="IsEnabled" Value="True" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding toolType, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}" Value="CommandGrid">
                        <Setter Property="Content" Value="" />
                        <Setter Property="IsEnabled" Value="True" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding toolType}" Value="null">
                        <Setter Property="IsEnabled" Value="False" />
                    </DataTrigger>
                </Style.Triggers>
            </Style>
            <Style
                x:Key="upbtn"
                BasedOn="{StaticResource MaterialDesignIconButton}"
                TargetType="Button">
                <Style.Triggers>
                    <DataTrigger Binding="{Binding topPos, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Value="True">
                        <Setter Property="Visibility" Value="Hidden" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding topPos, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Value="False">
                        <Setter Property="Visibility" Value="Visible" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding ID, Mode=OneWay}" Value="1">
                        <Setter Property="Visibility" Value="Hidden" />
                    </DataTrigger>
                </Style.Triggers>
            </Style>
            <Style
                x:Key="downbtn"
                BasedOn="{StaticResource MaterialDesignIconButton}"
                TargetType="Button">
                <Style.Triggers>
                    <DataTrigger Binding="{Binding ID, Mode=OneWay}" Value="1">
                        <Setter Property="Visibility" Value="Hidden" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding ID, Mode=OneWay}" Value="2">
                        <Setter Property="Visibility" Value="Hidden" />
                    </DataTrigger>
                </Style.Triggers>
            </Style>
            <Style
                x:Key="nameClear"
                BasedOn="{StaticResource MaterialDesignIconButton}"
                TargetType="Button">
                <Style.Triggers>
                    <DataTrigger Binding="{Binding ID, Mode=OneWay}" Value="1">
                        <Setter Property="Visibility" Value="Hidden" />
                    </DataTrigger>
                </Style.Triggers>
            </Style>

            <Style
                x:Key="customHeaderField"
                BasedOn="{StaticResource MahApps.Styles.TextBox}"
                TargetType="TextBox">
                <Setter Property="IsEnabled" Value="False" />

                <Style.Triggers>
                    <DataTrigger Binding="{Binding FieldSel}" Value="Custom">
                        <Setter Property="IsEnabled" Value="True" />
                    </DataTrigger>
                </Style.Triggers>
            </Style>

            <Style x:Key="toolsSADock" TargetType="DockPanel">
                <Setter Property="DockPanel.Dock" Value="Top" />
            </Style>

            <Style x:Key="toolsSALabel" TargetType="Label">
                <Setter Property="Margin" Value="25,0,0,0" />
                <Setter Property="DockPanel.Dock" Value="Top" />
            </Style>

            <Style x:Key="toolsSAScroll" TargetType="ScrollViewer">
                <Setter Property="DockPanel.Dock" Value="Top" />
                <Setter Property="HorizontalScrollBarVisibility" Value="Disabled" />
                <Setter Property="VerticalScrollBarVisibility" Value="Auto" />
            </Style>

            <Style
                x:Key="dataGridRowStyle"
                BasedOn="{StaticResource MahApps.Styles.DataGridRow}"
                TargetType="DataGridRow">

                <Style.Triggers>

                    <MultiTrigger>
                        <MultiTrigger.Conditions>
                            <Condition Property="Controls:DataGridHelper.SelectionUnit" Value="FullRow" />
                            <Condition Property="IsSelected" Value="True" />
                        </MultiTrigger.Conditions>
                        <Setter Property="Background" Value="Transparent" />
                        <Setter Property="BorderBrush" Value="Transparent" />
                    </MultiTrigger>
                    <MultiTrigger>
                        <MultiTrigger.Conditions>
                            <Condition Property="Controls:DataGridHelper.SelectionUnit" Value="FullRow" />
                            <Condition Property="IsMouseOver" Value="True" />
                        </MultiTrigger.Conditions>
                        <Setter Property="Background" Value="Transparent" />
                        <Setter Property="BorderBrush" Value="Transparent" />
                    </MultiTrigger>
                    <!--  IsSelected  -->
                    <MultiTrigger>
                        <MultiTrigger.Conditions>
                            <Condition Property="Controls:DataGridHelper.SelectionUnit" Value="FullRow" />
                            <Condition Property="IsSelected" Value="True" />
                        </MultiTrigger.Conditions>
                        <Setter Property="Background" Value="Transparent" />
                        <Setter Property="BorderBrush" Value="Transparent" />
                    </MultiTrigger>

                    <!--  IsSelected and Selector.IsSelectionActive  -->
                    <MultiTrigger>
                        <MultiTrigger.Conditions>

                            <Condition Property="IsSelected" Value="True" />
                        </MultiTrigger.Conditions>
                        <Setter Property="Background" Value="Transparent" />

                        <Setter Property="BorderBrush" Value="Transparent" />
                    </MultiTrigger>
                </Style.Triggers>
            </Style>
            <Style
                x:Key="dataGridRowStyleLog"
                BasedOn="{StaticResource dataGridRowStyle}"
                TargetType="DataGridRow">
                <Setter Property="Background" Value="#FF363333" />
            </Style>
            <Style
                x:Key="dataGridRowStyleHistory"
                BasedOn="{StaticResource dataGridRowStyle}"
                TargetType="DataGridRow">
                <Style.Triggers>

                    <Trigger Property="ItemsControl.AlternationIndex" Value="0">
                        <Setter Property="Background" Value="{StaticResource MahApps.Brushes.Accent3}" />
                    </Trigger>
                    <Trigger Property="ItemsControl.AlternationIndex" Value="1">
                        <Setter Property="Background" Value="{StaticResource MahApps.Brushes.Accent4}" />
                    </Trigger>
                </Style.Triggers>
            </Style>

            <Style
                x:Key="dataGridRowResultsStyleLog"
                BasedOn="{StaticResource dataGridRowStyle}"
                TargetType="DataGridRow">
                <Setter Property="Background" Value="Transparent" />
            </Style>

            <Style
                x:Key="dataGridCell"
                BasedOn="{StaticResource MaterialDesignDataGridCell}"
                TargetType="DataGridCell">
                <Style.Setters>
                    <Setter Property="BorderThickness" Value="0" />

                    <Setter Property="IsTabStop" Value="False" />
                </Style.Setters>
                <Style.Triggers>
                    <Trigger Property="IsSelected" Value="True">
                        <Setter Property="Background" Value="Transparent" />

                    </Trigger>
                </Style.Triggers>
            </Style>

            <Style
                x:Key="dataGridCellLog"
                BasedOn="{StaticResource dataGridCell}"
                TargetType="DataGridCell">
                <Style.Setters>

                    <Setter Property="Background" Value="#FF363333" />
                </Style.Setters>
            </Style>
            <Style
                x:Key="dataGridCellResultsLog"
                BasedOn="{StaticResource dataGridCell}"
                TargetType="DataGridCell">
                <Style.Setters>
                    <Setter Property="Background" Value="Transparent" />
                </Style.Setters>
            </Style>
            <Style
                x:Key="itemBox"
                BasedOn="{StaticResource MaterialDesignTextBox}"
                TargetType="{x:Type TextBox}">

                <Setter Property="BorderThickness" Value="0" />
                <Setter Property="FontSize" Value="16" />
                <Setter Property="Grid.Column" Value="0" />
                <Setter Property="md:HintAssist.Hint" Value="[empty]" />
                <Setter Property="md:HintAssist.HintOpacity" Value=".15" />
                <Setter Property="MaxLength" Value="20" />
                <Setter Property="FontSize" Value="16" />
                <Setter Property="FontFamily" Value="Segoe UI" />
                <Setter Property="md:TextFieldAssist.UnderlineCornerRadius" Value="5" />
                <Setter Property="md:TextFieldAssist.UnderlineBrush" Value="{StaticResource MahApps.Brushes.SystemControlBackgroundBaseMediumLow}" />
                <Setter Property="md:TextFieldAssist.NewSpecHighlightingEnabled" Value="False" />
                <Setter Property="Controls:TextBoxHelper.AutoWatermark" Value="True" />
                <Setter Property="Controls:TextBoxHelper.Watermark" Value="Null" />
                <Setter Property="md:TextFieldAssist.DecorationVisibility" Value="Visible" />
                <Setter Property="Margin" Value="3,10,0,0" />
                <Setter Property="CaretBrush" Value="AliceBlue" />

                <Style.Triggers>
                    <Trigger Property="Tag" Value="NoEdit">
                        <Setter Property="md:TextFieldAssist.DecorationVisibility" Value="Hidden" />
                        <Setter Property="md:TextFieldAssist.NewSpecHighlightingEnabled" Value="False" />
                        <Setter Property="IsReadOnly" Value="True" />
                        <Setter Property="Cursor" Value="Arrow" />
                    </Trigger>
                    <MultiTrigger>
                        <MultiTrigger.Conditions>
                            <Condition Property="IsMouseOver" Value="True" />
                            <Condition Property="IsReadOnly" Value="False" />
                        </MultiTrigger.Conditions>
                        <MultiTrigger.Setters>
                            <Setter Property="md:TextFieldAssist.RippleOnFocusEnabled" Value="True" />
                            <Setter Property="md:RippleAssist.ClipToBounds" Value="True" />
                        </MultiTrigger.Setters>
                    </MultiTrigger>
                </Style.Triggers>
