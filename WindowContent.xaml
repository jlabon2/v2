                <Setter Property="md:HintAssist.HintOpacity" Value=".15" />
                <Setter Property="md:TextFieldAssist.UnderlineBrush" Value="AliceBlue" />
                <Setter Property="md:TextFieldAssist.NewSpecHighlightingEnabled" Value="True" />
                <Setter Property="md:TextFieldAssist.DecorationVisibility" Value="Visible" />
                <Setter Property="md:TextFieldAssist.DecorationVisibility" Value="Hidden" />
                <Setter Property="md:TextFieldAssist.NewSpecHighlightingEnabled" Value="False" />
                <Setter Property="IsReadOnly" Value="True" />
                <Setter Property="HorizontalAlignment" Value="Left" />
                <Setter Property="Cursor" Value="Arrow" />
            </Style>

            <Style TargetType="Controls:MetroWindow">
                <Setter Property="FontFamily" Value="Segoe UI" />
            </Style>

