                <Setter Property="Padding" Value="0" />
                <Setter Property="Margin" Value="10,0,10,0" />
                <Style.Triggers>
                    <Trigger Property="Text" Value="[N/A]">
                        <Setter Property="Foreground" Value="{StaticResource MahApps.Brushes.Gray}" />
                    </Trigger>

