            <Style x:Key="itemBorder" TargetType="{x:Type Border}">
                <Setter Property="Margin" Value="1" />
                <Setter Property="Padding" Value="1" />
                <Setter Property="Background" Value="{DynamicResource MahApps.Brushes.ThemeBackground}" />
                <Setter Property="BorderBrush" Value="{DynamicResource MahApps.Brushes.Accent}" />
                <Setter Property="BorderThickness" Value="1" />
                <Setter Property="CornerRadius" Value="3" />
            </Style>

            <Style x:Key="itemEditClip" TargetType="{x:Type Label}">
                <Setter Property="Margin" Value="-3,-3,0,0" />
                <Setter Property="HorizontalAlignment" Value="Left" />
                <Setter Property="VerticalAlignment" Value="Top" />
                <Setter Property="DockPanel.Dock" Value="Left" />
                <Setter Property="Foreground" Value="{StaticResource MahApps.Brushes.SystemControlBackgroundBaseMediumLow}" />
                <Setter Property="FontSize" Value="10" />
                <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                <Setter Property="Content" Value="" />
            </Style>

            <Style
                x:Key="itemButton"
                BasedOn="{StaticResource MaterialDesignRaisedButton}"
                TargetType="{x:Type Button}">
                <Setter Property="Grid.Column" Value="1" />
                <Setter Property="Height" Value="Auto" />
                <Setter Property="Width" Value="Auto" />
                <Setter Property="Height" Value="20" />
                <Setter Property="Width" Value="20" />
                <Setter Property="Margin" Value="3" />
                <Setter Property="Padding" Value="1.5" />
                <Setter Property="Foreground" Value="WhiteSmoke" />
                <Setter Property="FontSize" Value="14" />
                <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                <Setter Property="Background" Value="{DynamicResource MahApps.Brushes.Accent3}" />
                <Setter Property="BorderBrush" Value="{DynamicResource MahApps.Brushes.Accent}" />
                <Setter Property="DockPanel.Dock" Value="Left" />
            </Style>

            <Style
                x:Key="adHocConfirmButton"
                BasedOn="{StaticResource itemButton}"
                TargetType="{x:Type Button}">
                <Setter Property="Width" Value="60" />
                <Setter Property="Height" Value="40" />
                <Setter Property="Margin" Value="0,0,15,10" />
                <Setter Property="Content" Value="Confirm" />
                <Setter Property="FontFamily" Value="Segoe UI Light" />
            </Style>

            <Style
                x:Key="itemToolButton"
                BasedOn="{StaticResource itemButton}"
                TargetType="{x:Type Button}">
                <Setter Property="Width" Value="63" />
                <Setter Property="Height" Value="63" />
                <Setter Property="FontSize" Value="24" />
            </Style>

            <Style
                x:Key="standAloneButton"
                BasedOn="{StaticResource itemToolButton}"
                TargetType="{x:Type Button}">
                <Setter Property="Width" Value="100" />
                <Setter Property="Height" Value="100" />
                <Setter Property="FontSize" Value="32" />
            </Style>

            <Style
                x:Key="itemEditButton"
                BasedOn="{StaticResource itemButton}"
                TargetType="{x:Type Button}">
                <Setter Property="Margin" Value="2" />
                <Setter Property="Content" Value="" />
            </Style>

            <Style
                x:Key="itemToolLargeButton"
                BasedOn="{StaticResource itemButton}"
                TargetType="{x:Type Button}">
                <Setter Property="Width" Value="63" />
                <Setter Property="Height" Value="63" />
                <Setter Property="FontSize" Value="24" />
                <Setter Property="Width" Value="63" />
            </Style>
            <Style x:Key="scriptBlockToolTipBase" TargetType="Run">
                <Setter Property="Background" Value="Transparent" />
                <Setter Property="FontFamily" Value="Segoe UI Light" />
                <Setter Property="Foreground" Value="WhiteSmoke" />
            </Style>

            <Style
                x:Key="scriptBlockToolTipSeverity"
                BasedOn="{StaticResource scriptBlockToolTipBase}"
                TargetType="Run">
                <Setter Property="Background" Value="Transparent" />
                <Setter Property="FontFamily" Value="Segoe UI" />
                <Setter Property="Foreground" Value="LightYellow" />
                <Style.Triggers>
                    <Trigger Property="Text" Value="ParseError">
                        <Setter Property="Foreground" Value="IndianRed" />
                    </Trigger>
                    <Trigger Property="Text" Value="Warning">
                        <Setter Property="Foreground" Value="Yellow" />
                    </Trigger>
                </Style.Triggers>
            </Style>
            <Style x:Key="scriptBlockValidStatus" TargetType="Label">
                <Setter Property="Background" Value="Transparent" />
                <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                <Setter Property="Foreground" Value="Red" />
                <Setter Property="Content" Value="" />
                <Setter Property="Panel.ZIndex" Value="1" />
                <Setter Property="Visibility" Value="Collapsed" />

                <Style.Triggers>
                    <Trigger Property="Tag" Value="True">
                        <Setter Property="Foreground" Value="LightGreen" />
                        <Setter Property="Content" Value="" />
                        <Setter Property="ToolTip" Value="Scriptblock valid" />
                        <Setter Property="Visibility" Value="Visible" />
                    </Trigger>
                    <Trigger Property="Tag" Value="False">
                        <Setter Property="Foreground" Value="Red" />
                        <Setter Property="Content" Value="" />
                        <Setter Property="ToolTip" Value="Scriptblock invalid" />
                        <Setter Property="Visibility" Value="Visible" />
                    </Trigger>
                </Style.Triggers>
            </Style>

            <Style x:Key="statusIconOnline" TargetType="{x:Type Label}">
                <Style.Setters>
                    <Setter Property="HorizontalAlignment" Value="Center" />
                    <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                </Style.Setters>
                <Style.Triggers>

                    <DataTrigger Binding="{Binding Connectivity, Mode=OneWay}" Value="True">

                        <Setter Property="Content" Value="" />
                        <Setter Property="Foreground" Value="LightGreen" />
                    </DataTrigger>

                    <DataTrigger Binding="{Binding Connectivity, Mode=OneWay}" Value="False">

                        <Setter Property="Content" Value="" />
                        <Setter Property="Foreground" Value="IndianRed" />
                    </DataTrigger>
                </Style.Triggers>
            </Style>
            <Style x:Key="statusIconActive" TargetType="{x:Type Label}">
                <Style.Setters>
                    <Setter Property="HorizontalAlignment" Value="Center" />
                    <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                </Style.Setters>
                <Style.Triggers>

                    <DataTrigger Binding="{Binding userOnline, Mode=OneWay}" Value="True">

                        <Setter Property="Content" Value="" />
                        <Setter Property="Foreground" Value="LightGreen" />
                        <Setter Property="ToolTip" Value="Session exists for user" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding userOnline, Mode=OneWay}" Value="Failed">

                        <Setter Property="Content" Value="" />
                        <Setter Property="Foreground" Value="Yellow" />
                        <Setter Property="ToolTip" Value="Could not determine session" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding userOnline, Mode=OneWay}" Value="False">

                        <Setter Property="Content" Value="" />
                        <Setter Property="Foreground" Value="IndianRed" />
                        <Setter Property="ToolTip" Value="No session exists for user" />
                    </DataTrigger>
                </Style.Triggers>
            </Style>
            <Style x:Key="statusClientIconOnline" TargetType="{x:Type Label}">
                <Style.Setters>
                    <Setter Property="HorizontalAlignment" Value="Left" />
                    <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
                </Style.Setters>
                <Style.Triggers>

                    <DataTrigger Binding="{Binding ClientOnline, Mode=OneWay}" Value="True">

                        <Setter Property="Content" Value="" />
                        <Setter Property="Foreground" Value="LightGreen" />
                    </DataTrigger>

                    <DataTrigger Binding="{Binding ClientOnline, Mode=OneWay}" Value="False">

                        <Setter Property="Content" Value="" />
                        <Setter Property="Foreground" Value="IndianRed" />
                    </DataTrigger>
                </Style.Triggers>
            </Style>

            <Style x:Key="headerBlocks" TargetType="TextBlock">
                <Setter Property="FontFamily" Value="Segoe UI Light" />
                <Setter Property="VerticalAlignment" Value="Center" />
                <Setter Property="FontSize" Value="12" />
                <Setter Property="Margin" Value="5,0,5,0" />
                <Setter Property="Visibility" Value="Visible" />
                <Style.Triggers>

                    <Trigger Property="Text" Value="">
                        <Setter Property="Visibility" Value="Collapsed" />
                    </Trigger>
                </Style.Triggers>
            </Style>
        </ResourceDictionary>
    </Window.Resources>

    <Controls:MetroWindow.RightWindowCommands>

        <Controls:WindowCommands
            Name="headerControl"
            Margin="0,-2.5,0,0"
            HorizontalAlignment="Right"
            VerticalAlignment="Center"
            ShowLastSeparator="True">
            <Button
                Name="headerConfigUpdate"
                Width="200"
                Height="25"
                Padding="0"
                Cursor="Hand"
                FontWeight="Light"
                Visibility="Collapsed">
                <Button.Content>
                    <StackPanel Margin="35,0,0,0" Orientation="Horizontal">
                        <TextBlock
                            FontFamily="Segoe UI"
                            Foreground="LightGreen"
                            Style="{StaticResource headerBlocks}"
                            Text="Config update available" />
                        <TextBlock
                            Margin="0,3,0,0"
                            FontFamily="Segoe MDL2 Assets"
                            Text="" />
                    </StackPanel>
                </Button.Content>
            </Button>
            <TextBlock Name="headerUser" Style="{StaticResource headerBlocks}" />
            <TextBlock
                Foreground="{Binding headerColor}"
                Style="{StaticResource headerBlocks}"
                Text="{Binding headerAdd}" />
        </Controls:WindowCommands>
    </Controls:MetroWindow.RightWindowCommands>

    <Grid>
        <Grid Name="splashLoad" Visibility="Visible">
            <DockPanel
                x:Name="settingPopup"
                HorizontalAlignment="Center"
                VerticalAlignment="Top"
                Panel.ZIndex="1">
                <Label
                    Name="settingADRegenText"
                    Margin="0,-25,0,25"
                    HorizontalAlignment="Center"
                    Panel.ZIndex="5"
                    Content="Generating initial Active Directory mappings... this may take a minute."
                    DockPanel.Dock="Bottom"
                    FontFamily="Segoe UI Light"
                    FontSize="12"
                    Visibility="Hidden" />
                <ProgressBar
                    x:Name="settingProgressBar"
                    Width="400"
                    Height="10"
                    Margin="0,0,0,50"
                    HorizontalAlignment="Center"
                    Panel.ZIndex="5"
                    DockPanel.Dock="Bottom"
                    IsIndeterminate="True"
                    Style="{StaticResource MaterialDesignLinearProgressBar}"
                    Visibility="Visible" />
                <Image
                    Name="settingLogo"
                    Margin="50"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Bottom"
                    Panel.ZIndex="2"
                    DockPanel.Dock="Top"
                    Opacity=".5"
                    Visibility="Visible" />
            </DockPanel>
        </Grid>

        <Grid Name="windowContent">
            <Canvas
                Height="300"
                Margin="40,0,0,75"
                HorizontalAlignment="Left"
                VerticalAlignment="Bottom"
                Panel.ZIndex="2"
                Background="Transparent">
                <Controls:Flyout
                    Name="historySidePane"
                    Width="600"
                    Height="325"
                    Margin="17.5,0,0,20"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Center"
                    Panel.ZIndex="-10"
                    Background="#FF444444"
                    CloseButtonVisibility="Collapsed"
                    IsOpen="False"
                    Position="Left">

                    <StackPanel Margin="10,-40,10,0">
                        <DockPanel>
                            <Label
                                Margin="-30,0,0,0"
                                HorizontalAlignment="Center"
                                Controls:ControlsHelper.ContentCharacterCasing="Upper"
                                Content="History"
                                DockPanel.Dock="Right"
                                FontFamily="{DynamicResource MahApps.Fonts.Family.Header}"
                                FontSize="18">
                                <Label.Effect>
                                    <DropShadowEffect />
                                </Label.Effect>
                            </Label>
                        </DockPanel>
                        <DockPanel>
                            <DataGrid
                                Name="historySideDataGrid"
                                Grid.Row="0"
                                Width="580"
                                Height="285"
                                Padding="0"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Stretch"
                                Controls:DataGridHelper.SelectionUnit="FullRow"
                                md:DataGridAssist.CellPadding="-3,0,-3,0"
                                md:DataGridAssist.ColumnHeaderPadding="5,0,5,0"
                                md:ListViewAssist.ListViewItemPadding="1"
                                AlternationCount="2"
                                AutoGenerateColumns="False"
                                Background="Transparent"
                                BorderBrush="#FF444444"
                                BorderThickness="1,0,1,1"
                                CanUserAddRows="False"
                                CanUserDeleteRows="False"
                                CanUserReorderColumns="False"
                                CanUserResizeColumns="False"
                                CanUserResizeRows="False"
                                CanUserSortColumns="False"
                                CellStyle="{StaticResource dataGridCellResultsLog}"
                                DockPanel.Dock="Top"
                                Focusable="True"
                                GridLinesVisibility="All"
                                HorizontalScrollBarVisibility="Disabled"
                                IsReadOnly="True"
                                KeyboardNavigation.TabNavigation="Cycle"
                                RowHeaderWidth="0"
                                RowHeight="30"
                                RowStyle="{StaticResource dataGridRowStyleHistory}"
                                ScrollViewer.CanContentScroll="False"
                                SelectionMode="Single"
                                VerticalScrollBarVisibility="Visible">

                                <DataGrid.ColumnHeaderStyle>
                                    <Style BasedOn="{StaticResource MahApps.Styles.DataGridColumnHeader}" TargetType="DataGridColumnHeader">
                                        <Setter Property="Background" Value="Transparent" />
                                        <Setter Property="Margin" Value="0,0,0,-25" />
                                    </Style>
                                </DataGrid.ColumnHeaderStyle>

                                <DataGrid.Columns>
                                    <DataGridTemplateColumn
                                        Width="100"
                                        Header="ACTION"
                                        HeaderStyle="{StaticResource historyGridHeader}">

                                        <DataGridTemplateColumn.CellTemplate>

                                            <DataTemplate>
                                                <Viewbox HorizontalAlignment="Left" StretchDirection="DownOnly">
                                                    <TextBlock Style="{StaticResource toolLabel}" Text="{Binding ActionName}" />
                                                </Viewbox>
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellTemplate>
                                    </DataGridTemplateColumn>

                                    <DataGridTemplateColumn
                                        Width="70"
                                        Header="RESULT"
                                        HeaderStyle="{StaticResource historyGridHeader}">

                                        <DataGridTemplateColumn.CellTemplate>

                                            <DataTemplate>
                                                <Viewbox HorizontalAlignment="Left" StretchDirection="DownOnly">
                                                    <TextBlock
                                                        Background="Transparent"
                                                        Style="{StaticResource toolLabel}"
                                                        Text="{Binding Message}" />
                                                </Viewbox>
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellTemplate>
                                    </DataGridTemplateColumn>

                                    <DataGridTemplateColumn
                                        Width="100"
                                        Header="TARGET"
                                        HeaderStyle="{StaticResource historyGridHeader}">

                                        <DataGridTemplateColumn.CellTemplate>

                                            <DataTemplate>
                                                <Viewbox HorizontalAlignment="Left" StretchDirection="DownOnly">
                                                    <TextBlock Style="{StaticResource toolLabel}" Text="{Binding SubjectName}" />
                                                </Viewbox>
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellTemplate>
                                    </DataGridTemplateColumn>
                                    <DataGridTemplateColumn
                                        Width="60"
                                        Header="TYPE"
                                        HeaderStyle="{StaticResource historyGridHeader}">

                                        <DataGridTemplateColumn.CellTemplate>

                                            <DataTemplate>
                                                <Viewbox HorizontalAlignment="Left" StretchDirection="DownOnly">
                                                    <TextBlock
                                                        VerticalAlignment="Center"
                                                        Style="{StaticResource toolLabel}"
                                                        Text="{Binding SubjectType}" />
                                                </Viewbox>
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellTemplate>
                                    </DataGridTemplateColumn>
                                    <DataGridTemplateColumn
                                        Width="70"
                                        Header="DATE"
                                        HeaderStyle="{StaticResource historyGridHeader}">

                                        <DataGridTemplateColumn.CellTemplate>

                                            <DataTemplate>
                                                <Viewbox HorizontalAlignment="Left" StretchDirection="DownOnly">
                                                    <TextBlock Style="{StaticResource toolLabel}" Text="{Binding Date}" />
                                                </Viewbox>
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellTemplate>
                                    </DataGridTemplateColumn>
                                    <DataGridTemplateColumn
                                        Width="70"
                                        Header="TIME"
                                        HeaderStyle="{StaticResource historyGridHeader}">

                                        <DataGridTemplateColumn.CellTemplate>

                                            <DataTemplate>
                                                <Viewbox HorizontalAlignment="Left" StretchDirection="DownOnly">
                                                    <TextBlock Style="{StaticResource toolLabel}" Text="{Binding Time}" />
                                                </Viewbox>
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellTemplate>
                                    </DataGridTemplateColumn>

                                    <DataGridTemplateColumn
                                        Width="50"
                                        Header=""
                                        HeaderStyle="{StaticResource historyGridHeader}">
                                        <DataGridTemplateColumn.CellTemplate>
                                            <DataTemplate>
                                                <Button
                                                    x:Name="resultsErrorItem"
                                                    Width="15"
                                                    Height="15"
                                                    Margin="-8,-7,0,-7"
                                                    Padding="0"
                                                    HorizontalAlignment="Center"
                                                    FontFamily="Segoe MDL2 Assets"
                                                    FontSize="14"
                                                    IsTabStop="False">
                                                    <Button.Style>
                                                        <Style BasedOn="{StaticResource MaterialDesignToolButton}" TargetType="Button">

                                                            <Setter Property="Content" Value="" />

                                                            <Setter Property="ToolTip">
                                                                <Setter.Value>
                                                                    <TextBlock MaxWidth="200" TextWrapping="Wrap">
                                                                        <Run Foreground="IndianRed" Text="{Binding Error}" />
                                                                        <LineBreak />
                                                                        <LineBreak />
                                                                        <Run Text="Select to copy error" />
                                                                    </TextBlock>
                                                                </Setter.Value>
                                                            </Setter>
                                                            <Setter Property="Foreground" Value="IndianRed" />
                                                            <Style.Triggers>
                                                                <DataTrigger Binding="{Binding Error}" Value="[none]">
                                                                    <Setter Property="Content" Value="" />
                                                                    <Setter Property="ToolTip" Value="No error" />
                                                                    <Setter Property="md:RippleAssist.IsDisabled" Value="False" />
                                                                    <Setter Property="Cursor" Value="Arrow" />
                                                                    <Setter Property="Foreground" Value="LightGreen" />
                                                                </DataTrigger>

                                                                <MultiTrigger>
                                                                    <MultiTrigger.Conditions>
                                                                        <Condition Property="IsMouseOver" Value="True" />
                                                                        <Condition Property="Content" Value="" />
                                                                    </MultiTrigger.Conditions>
                                                                    <Setter Property="Foreground" Value="{StaticResource MahApps.Brushes.Accent}" />
                                                                </MultiTrigger>
                                                            </Style.Triggers>
                                                        </Style>
                                                    </Button.Style>
                                                </Button>
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellTemplate>
                                    </DataGridTemplateColumn>

                                    <DataGridTemplateColumn Width="50" HeaderStyle="{StaticResource datagridColumnNoTab}">
                                        <DataGridTemplateColumn.CellTemplate>
                                            <DataTemplate>
                                                <Button
                                                    x:Name="resultsQueryItem"
                                                    Width="15"
                                                    Height="15"
                                                    Margin="-10,-7,0,-7"
                                                    Padding="0"
                                                    HorizontalAlignment="Center"
                                                    Content=""
                                                    FontFamily="Segoe MDL2 Assets"
                                                    FontSize="14"
                                                    IsTabStop="False">
                                                    <Button.Style>
                                                        <Style BasedOn="{StaticResource MaterialDesignToolButton}" TargetType="Button">
                                                            <Setter Property="Content" Value="" />
                                                            <Setter Property="FontSize" Value="14" />
                                                            <Setter Property="ToolTip" Value="Search object" />
                                                            <Setter Property="Foreground" Value="Silver" />
                                                            <Setter Property="IsEnabled" Value="True" />
                                                            <Style.Triggers>
                                                                <DataTrigger Binding="{Binding SubjectName, UpdateSourceTrigger=PropertyChanged}" Value="[N/A]">
                                                                    <Setter Property="IsEnabled" Value="False" />
                                                                </DataTrigger>
                                                                <Trigger Property="IsMouseOver" Value="True">
                                                                    <Setter Property="Foreground" Value="{StaticResource MahApps.Brushes.Accent}" />
                                                                </Trigger>
                                                            </Style.Triggers>
                                                        </Style>
                                                    </Button.Style>
                                                </Button>
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellTemplate>
                                    </DataGridTemplateColumn>
                                </DataGrid.Columns>
                            </DataGrid>
                        </DockPanel>
                    </StackPanel>
                </Controls:Flyout>
                <DockPanel
                    Height="325"
                    Margin="-50,0,0,20"
                    HorizontalAlignment="Left"
