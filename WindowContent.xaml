                <Setter Property="VerticalAlignment" Value="Center" />
                <Setter Property="HorizontalAlignment" Value="Left" />
                <Setter Property="Margin" Value="0,0,2,0" />

                <Setter Property="StretchDirection" Value="DownOnly" />
                <Setter Property="DockPanel.Dock" Value="Top" />
            </Style>

            <Style
                x:Key="compItemBox"
                BasedOn="{StaticResource MaterialDesignTextBoxBase}"
                TargetType="{x:Type TextBox}">
                <Setter Property="Background" Value="Transparent" />
                <Setter Property="md:TextFieldAssist.UnderlineBrush" Value="Transparent" />
                <Setter Property="md:TextFieldAssist.DecorationVisibility" Value="Hidden" />
                <Setter Property="md:TextFieldAssist.NewSpecHighlightingEnabled" Value="False" />
